{
    "openapi": "3.0.0",
    "paths": {
        "/": {
            "get": {
                "operationId": "AppController_getHello",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": ""
                    }
                }
            }
        },
        "/members/register": {
            "post": {
                "operationId": "MembersController_register",
                "parameters": [],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RegisterDto"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "註冊成功",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/RegisterResponseDto"
                                }
                            }
                        }
                    },
                    "409": {
                        "description": "註冊失敗",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponseDto"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "members"
                ]
            }
        },
        "/members/login": {
            "post": {
                "operationId": "MembersController_login",
                "parameters": [],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginDto"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "登入成功",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/LoginResponseDto"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "登入失敗",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponseDto"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "members"
                ]
            }
        },
        "/patients": {
            "post": {
                "operationId": "PatientsController_create",
                "parameters": [],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CreatePatientDto"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "病患資料建立成功",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PatientResponseDto"
                                }
                            }
                        }
                    },
                    "409": {
                        "description": "該病患資料已經存在",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ErrorResponseDto"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "patients"
                ]
            },
            "get": {
                "operationId": "PatientsController_searchAll",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "取得所有病患資料",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/PatientSearchResponseDto"
                                    }
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "patients"
                ]
            }
        }
    },
    "info": {
        "title": "EUCare API",
        "description": "EUCare API description",
        "version": "1.0",
        "contact": {}
    },
    "tags": [
        {
            "name": "EUCare",
            "description": ""
        }
    ],
    "servers": [],
    "components": {
        "schemas": {
            "RegisterDto": {
                "type": "object",
                "properties": {
                    "phoneNumber": {
                        "type": "string",
                        "example": "0912345678",
                        "description": "電話號碼"
                    },
                    "password": {
                        "type": "string",
                        "example": "123456",
                        "description": "密碼"
                    }
                },
                "required": [
                    "phoneNumber",
                    "password"
                ]
            },
            "RegisterResponseDto": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "number",
                        "example": 1,
                        "description": "用戶ID"
                    },
                    "phoneNumber": {
                        "type": "string",
                        "example": "0912345678",
                        "description": "電話號碼"
                    }
                },
                "required": [
                    "id",
                    "phoneNumber"
                ]
            },
            "ErrorResponseDto": {
                "type": "object",
                "properties": {
                    "statusCode": {
                        "type": "number",
                        "example": 409,
                        "description": "HTTP 狀態碼"
                    },
                    "message": {
                        "type": "string",
                        "example": "該病患資料已經存在",
                        "description": "錯誤訊息"
                    },
                    "error": {
                        "type": "string",
                        "example": "Conflict",
                        "description": "錯誤描述"
                    }
                },
                "required": [
                    "statusCode",
                    "message",
                    "error"
                ]
            },
            "LoginDto": {
                "type": "object",
                "properties": {
                    "phoneNumber": {
                        "type": "string",
                        "example": "0912345678",
                        "description": "電話號碼"
                    },
                    "password": {
                        "type": "string",
                        "example": "123456",
                        "description": "密碼"
                    }
                },
                "required": [
                    "phoneNumber",
                    "password"
                ]
            },
            "LoginResponseDto": {
                "type": "object",
                "properties": {
                    "accessToken": {
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...",
                        "description": "JWT Access Token"
                    }
                },
                "required": [
                    "accessToken"
                ]
            },
            "CreatePatientDto": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "林小銘",
                        "description": "病患姓名"
                    },
                    "identityNo": {
                        "type": "string",
                        "example": "A123456789",
                        "description": "身分證字號"
                    },
                    "birthdate": {
                        "type": "string",
                        "example": "1980-01-01",
                        "description": "出生年月日"
                    },
                    "address": {
                        "type": "string",
                        "example": "台北市",
                        "description": "住址"
                    }
                },
                "required": [
                    "name",
                    "identityNo",
                    "birthdate",
                    "address"
                ]
            },
            "PatientResponseDto": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "number",
                        "example": 15,
                        "description": "病患 ID"
                    },
                    "name": {
                        "type": "string",
                        "example": "林小銘",
                        "description": "病患姓名"
                    },
                    "identityNo": {
                        "type": "string",
                        "example": "A123456789",
                        "description": "身分證字號"
                    },
                    "birthdate": {
                        "type": "string",
                        "example": "1980-01-01",
                        "description": "出生年月日"
                    },
                    "address": {
                        "type": "string",
                        "example": "台北市",
                        "description": "住址"
                    },
                    "memberId": {
                        "type": "number",
                        "example": 4,
                        "description": "會員 ID"
                    }
                },
                "required": [
                    "id",
                    "name",
                    "identityNo",
                    "birthdate",
                    "address",
                    "memberId"
                ]
            },
            "PatientSearchResponseDto": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "林小銘",
                        "description": "病患姓名"
                    },
                    "identityNo": {
                        "type": "string",
                        "example": "A123456789",
                        "description": "身分證字號"
                    },
                    "birthdate": {
                        "type": "string",
                        "example": "1980-01-01",
                        "description": "出生年月日"
                    },
                    "address": {
                        "type": "string",
                        "example": "台北市",
                        "description": "住址"
                    }
                },
                "required": [
                    "name",
                    "identityNo",
                    "birthdate",
                    "address"
                ]
            }
        }
    }
}